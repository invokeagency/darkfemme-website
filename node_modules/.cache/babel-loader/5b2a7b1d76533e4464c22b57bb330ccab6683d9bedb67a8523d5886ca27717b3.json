der-radius: 12px;\n  padding: 12px 24px;\n  font-family: 'Montserrat', sans-serif;\n  font-size: 14px;\n  font-weight: 500;\n  letter-spacing: 1px;\n  cursor: pointer;\n  transition: background-color 0.2s;\n}\n\n.button-outline:hover {\n  background-color: rgba(178, 34, 34, 0.1);\n}\n\n/* Features Section */\n.features-section {\n  display: grid;\n  grid-template-columns: repeat(3, 1fr);\n  gap: 1.5rem;\n  padding: 0 1rem;\n}\n\n.feature-card {\n  background: linear-gradient(to bottom right, var(--card-gradient-start), var(--card-gradient-end));\n  border-radius: 16px;\n  border: 1px solid var(--card-border);\n  padding: 1.5rem;\n  text-align: left;\n  box-shadow: 0 8px 16px var(--card-shadow);\n  transition: all 0.3s ease;\n}\n\n.feature-card:hover {\n  transform: translateY(-2px);\n  box-shadow: \n    0 10px 20px rgba(178, 34, 34, 0.4),\n    0 0 30px rgba(59, 10, 69, 0.2);\n}\n\n.feature-icon {\n  font-size: 1.5rem;\n  margin-bottom: 1rem;\n  color: var(--crimson-red);\n}\n\n.feature-card h3 {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1.1rem;\n  font-weight: 500;\n  margin-bottom: 0.5rem;\n  color: var(--ivory);\n}\n\n.feature-card p {\n  font-size: 0.9rem;\n  color: var(--smoky-silver);\n}\n\n/* Footer */\nfooter {\n  padding: 2vh 0;\n}\n\n.footer-content {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-size: 0.8rem;\n}\n\n.footer-links {\n  display: flex;\n  gap: 1.5rem;\n}\n\n.footer-links a {\n  text-decoration: none;\n  transition: color 0.3s ease;\n}\n\n.text-silver {\n  color: var(--smoky-silver);\n}\n\n/* Responsive */\n@media (max-width: 768px) {\n  .content-container {\n    padding: 2vh 1rem;\n  }\n  \n  .hero-section {\n    text-align: center;\n    padding-left: 0;\n  }\n  \n  .accent-line {\n    margin: 1rem auto;\n  }\n  \n  .power-text {\n    font-size: 2rem;\n  }\n  \n  .features-section {\n    grid-template-columns: 1fr;\n    gap: 1rem;\n  }\n  \n  .cta-buttons {\n    flex-direction: column;\n    