s":["/Users/fgk/Developer/Invoke/darkfemme-website/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"app gradient-background\">\n    <canvas id=\"particleCanvas\" class=\"particle-container\"></canvas>\n    \n    <div class=\"content-container\">\n      <div class=\"content-wrapper\">\n        <div class=\"hero-section\">\n          <h1 class=\"brand-name\">DARK FEMME</h1>\n          <div class=\"accent-line\"></div>\n          <h2 class=\"power-text\">Your power awaits.</h2>\n          <div class=\"cta-buttons\">\n            <button class=\"button-primary\">\n              Download on the App Store\n              <span class=\"button-arrow\">→</span>\n            </button>\n            <button class=\"button-outline\">Contact Us</button>\n          </div>\n        </div>\n        \n        <div class=\"features-section\">\n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <span class=\"icon-moon\">◗</span>\n            </div>\n            <h3>Daily Mystique</h3>\n            <p>Transform your life with daily practices</p>\n          </div>\n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <span class=\"icon-crown\">◈</span>\n            </div>\n            <h3>Premium Coaching</h3>\n            <p>Access personalized guidance</p>\n          </div>\n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <span class=\"icon-star\">✧</span>\n            </div>\n            <h3>Structured Learning</h3>\n            <p>Master the art of feminine energy</p>\n          </div>\n        </div>\n      </div>\n      \n      <footer>\n        <div class=\"footer-content\">\n          <p class=\"text-silver\">© 2024 Dark Femme. All rights reserved.</p>\n          <div class=\"footer-links\">\n            <a href=\"#\" class=\"text-silver\">Privacy Policy</a>\n            <a href=\"#\" class=\"text-silver\">Terms of Service</a>\n          </div>\n        </div>\n      </footer>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'App',\n  mounted() {\n    class ParticleSystem {\n      constructor(canvas) {\n        this.canvas = canvas;\n        this.ctx = canvas.getContext('2d');\n        this.particles = [];\n        this.colors = [\n          'rgba(178, 34, 34, 0.6)',  // Crimson\n          'rgba(59, 10, 69, 0.6)',   // Aubergine\n          'rgba(166, 166, 166, 0.6)' // Silver\n        ];\n        \n        this.init();\n        this.animate();\n        \n        window.addEventListener('resize', () => this.handleResize());\n        this.handleResize();\n      }\n      \n      init() {\n        for (let i = 0; i < 15; i++) {\n          this.particles.push({\n            x: Math.random() * this.canvas.width,\n            y: Math.random() * this.canvas.height,\n            size: 1 + Math.random(),\n            color: this.colors[i % this.colors.length],\n            shape: ['diamond', 'circle', 'plus'][i % 3],\n            speedX: (Math.random() - 0.5) * 0.5,\n            speedY: (Math.random() - 0.5) * 0.5,\n            angle: Math.random() * 360,\n            rotationSpeed: (Math.random() - 0.5) * 0.02\n          });\n        }\n      }\n      \n      handleResize() {\n        this.canvas.width = window.innerWidth;\n        this.canvas.height = window.innerHeight;\n      }\n      \n      drawParticle(particle) {\n        this.ctx.save();\n        this.ctx.translate(particle.x, particle.y);\n        this.ctx.rotate(particle.angle);\n        this.ctx.fillStyle = particle.color;\n        \n        switch (particle.shape) {\n          case 'diamond':\n            this.ctx.beginPath();\n            this.ctx.moveTo(0, -particle.size);\n            this.ctx.lineTo(particle.size, 0);\n            this.ctx.lineTo(0, particle.size);\n            this.ctx.lineTo(-particle.size, 0);\n            this.ctx.closePath();\n            this.ctx.fill();\n            break;\n            \n          case 'plus':\n            this.ctx.fillRect(-particle.size, -particle.size * 0.3, particle.size * 2, particle.size * 0.6);\n            this.ctx.fillRect(-particle.size * 0.3, -particle.size, particle.size * 0.6, particle.size * 2);\n            break;\n            \n          case 'circle':\n            this.ctx.beginPath();\n            this.ctx.arc(0, 0, particle.size, 0, Math.PI * 2);\n            this.ctx.fill();\n            break;\n        }\n        \n        this.ctx.restore();\n      }\n      \n      animate() {\n        this.ctx.clearRect(0, 0, this.canvas.width, this.canvas.height);\n        \n        this.particles.forEach(particle => {\n          particle.x += particle.speedX;\n          particle.y += particle.speedY;\n          particle.angle += particle.rotationSpeed;\n          \n          if (particle.x < 0) particle.x = this.canvas.width;\n          if (particle.x > this.canvas.width) particle.x = 0;\n          if (particle.y < 0) particle.y = this.canvas.height;\n          if (particle.y > this.canvas.height) particle.y = 0;\n          \n          this.drawParticle(particle);\n        });\n        \n        requestAnimationFrame(() => this.animate());\n      }\n    }\n\n    const canvas = document.getElementById('particleCanvas');\n    if (canvas) {\n      new ParticleSystem(canvas);\n    }\n  }\n}\n</script>\n\n<style>\n:root {\n  --deep-noir: #0D0D0D;\n  --crimson-red: #B22222;\n  --rich-aubergine: #3B0A45;\n  --soft-charcoal: #2E2E2E;\n  --smoky-silver: #A6A6A6;\n  --ivory: #F5F5F5;\n  \n  /* Additional colors for gradients */\n  --card-gradient-start: #1A1A1A;\n  --card-gradient-end: #0D0D0D;\n  --card-border: rgba(59, 10, 69, 0.3);\n  --card-shadow: rgba(178, 34, 34, 0.3);\n}\n\n/* Base Styles */\n@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600&display=swap');\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nhtml, body {\n  height: 100%;\n  overflow: hidden;\n}\n\nbody {\n  background-color: var(--deep-noir);\n  font-family: 'Montserrat', sans-serif;\n  font-size: 16px;\n  color: var(--ivory);\n  letter-spacing: 0.5px;\n  line-height: 1.6;\n}\n\n.app {\n  height: 100vh;\n  display: flex;\n  background: radial-gradient(\n    circle at top right,\n    rgba(178, 34, 34, 0.1),\n    rgba(59, 10, 69, 0.05),\n    var(--deep-noir)\n  );\n}\n\n.content-container {\n  position: relative;\n  z-index: 1;\n  width: 100%;\n  height: 100%;\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 2vh 2rem;\n  display: flex;\n  flex-direction: column;\n}\n\n.content-wrapper {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  gap: 4vh;\n}\n\n/* Hero Section */\n.hero-section {\n  text-align: left;\n  padding-left: 1rem;\n}\n\n.brand-name {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1.2rem;\n  font-weight: 500;\n  letter-spacing: 2px;\n  color: var(--crimson-red);\n  margin-bottom: 0.5rem;\n}\n\n.power-text {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 2.5rem;\n  font-weight: 300;\n  color: var(--ivory);\n  margin: 1rem 0;\n  letter-spacing: 0.5px;\n}\n\n.accent-line {\n  width: 40px;\n  height: 1px;\n  background: linear-gradient(\n    to right,\n    var(--crimson-red),\n    rgba(178, 34, 34, 0.3)\n  );\n  margin: 1rem 0;\n}\n\n/* Buttons */\n.cta-buttons {\n  display: flex;\n  gap: 1rem;\n  margin-top: 2rem;\n}\n\n.button-primary {\n  background: linear-gradient(135deg, var(--crimson-red), #8B0000);\n  color: var(--ivory);\n  border: none;\n  border-radius: 12px;\n  padding: 12px 24px;\n  font-family: 'Montserrat', sans-serif;\n  font-size: 14px;\n  font-weight: 500;\n  letter-spacing: 1px;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  box-shadow: 0 4px 8px rgba(178, 34, 34, 0.5);\n  transition: all 0.2s ease;\n}\n\n.button-primary:hover {\n  transform: translateY(-1px);\n  box-shadow: 0 6px 12px rgba(178, 34, 34, 0.6);\n}\n\n.button-arrow {\n  font-size: 18px;\n  margin-left: 4px;\n}\n\n.button-outline {\n  background: transparent;\n  color: var(--crimson-red);\n  border: 1.5px solid var(--crimson-red);\n  border-radius: 12px;\n  padding: 12px 24px;\n  font-family: 'Montserrat', sans-serif;\n  font-size: 14px;\n  font-weight: 500;\n  letter-spacing: 1px;\n  cursor: pointer;\n  transition: background-color 0.2s;\n}\n\n.button-outline:hover {\n  background-color: rgba(178, 34, 34, 0.1);\n}\n\n/* Features Section */\n.features-section {\n  display: grid;\n  grid-template-columns: repeat(3, 1fr);\n  gap: 1.5rem;\n  padding: 0 1rem;\n}\n\n.feature-card {\n  background: linear-gradient(to bottom right, var(--card-gradient-start), var(--card-gradient-end));\n  border-radius: 16px;\n  border: 1px solid var(--card-border);\n  padding: 1.5rem;\n  text-align: left;\n  box-shadow: 0 8px 16px var(--card-shadow);\n  transition: all 0.3s ease;\n}\n\n.feature-card:hover {\n  transform: translateY(-2px);\n  box-shadow: \n    0 10px 20px rgba(178, 34, 34, 0.4),\n    0 0 30px rgba(59, 10, 69, 0.2);\n}\n\n.feature-icon {\n  font-size: 1.5rem;\n  margin-bottom: 1rem;\n  color: var(--crimson-red);\n}\n\n.feature-card h3 {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1.1rem;\n  font-weight: 500;\n  margin-bottom: 0.5rem;\n  color: var(--ivory);\n}\n\n.feature-card p {\n  font-size: 0.9rem;\n  color: var(--smoky-silver);\n}\n\n/* Footer */\nfooter {\n  padding: 2vh 0;\n}\n\n.footer-content {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-size: 0.8rem;\n}\n\n.footer-links {\n  display: flex;\n  gap: 1.5rem;\n}\n\n.footer-links a {\n  text-decoration: none;\n  transition: color 0.3s ease;\n}\n\n.text-silver {\n  color: var(--smoky-silver);\n}\n\n/* Responsive */\n@media (max-width: 768px) {\n  .content-container {\n    padding: 2vh 1rem;\n  }\n  \n  .hero-section {\n    text-align: center;\n    padding-left: 0;\n  }\n  \n  .accent-line {\n    margin: 1rem auto;\n  }\n  \n  .power-text {\n    font-size: 2rem;\n  }\n  \n  .features-section {\n    grid-template-columns: 1fr;\n    gap: 1rem;\n  }\n  \n  .cta-buttons {\n    flex-direction: column;\n    align-items: stretch;\n  }\n  \n  .footer-content {\n    flex-direction: column;\n    text-align: center;\n    gap: 1vh;\n  }\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAyB;;uBAApCC,mBAAA,CAqDM,OArDNC,UAqDM,EAAAC,MAAA,QAAAA,MAAA,O","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}